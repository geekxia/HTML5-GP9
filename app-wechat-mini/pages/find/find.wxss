/* pages/find/find.wxss */

.c1 {
  color: blue;
}
.c2 {
  color: orange;
}

/* 使用内置组件的标签名写样式 */
input {
  border: 1rpx solid #ccc;
  line-height: 60rpx;
  height: 60rpx;
  width: 700rpx;
  margin: 0 auto;
  /* text-indent: 30rpx; */
  padding-left: 30rpx;
  box-sizing: border-box;
  border-radius: 5rpx;
}

#foo {
  height: 200rpx;
  margin: 0 auto;
  width: 0;
  background-color: black;
}

.canvas {
  width: 100%;
  height: 500rpx;
}

.navs {
  white-space: nowrap;
}
.item {
  display: inline-block;
  line-height: 100rpx;
  padding: 10rpx 20rpx;
}
.item.on {
  color: red;
}

.map {
  width: 100%;
  height: 500rpx;
}